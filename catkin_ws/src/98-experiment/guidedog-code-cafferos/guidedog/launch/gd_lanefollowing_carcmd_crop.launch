<launch>
    <arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="local" default="false" doc="true for running on laptop. false for running on vehicle."/>

	<remap from="gd_crop_lanefollowing_carcmd_sync_node/image" to="camera_node/image/compressed"/>
	<remap from="gd_crop_lanefollowing_carcmd_sync_node/lane_pose" to="lane_filter_node/lane_pose"/>
	<remap from="gd_crop_lanefollowing_carcmd_sync_node/car_cmd" to="lane_controller_node/car_cmd"/>
    <remap from="gd_crop_lanefollowing_carcmd_sync_node/wheels_cmd" to="wheels_driver_node/wheels_cmd"/>
	<include file="$(find gd_analysis)/launch/gd_crop_lanefollowing_carcmd_sync_node.launch">
    	<arg name="veh" value="$(arg veh)"/>
    	<arg name="local" value="$(arg local)"/>
  	</include>

</launch>
